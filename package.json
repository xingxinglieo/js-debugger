{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "db": "node prisma/delete.js && npx prisma db push && node --require esbuild-register prisma/seed.ts",
    "db:dev": "npx prisma studio",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css --minify && postcss styles --base styles --dir app/styles --env production",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "concurrently \"tailwindcss -o ./app/tailwind.css --watch\" \"postcss styles --base styles --dir app/styles -w\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/generator": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/types": "^7.16.8",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@loadable/component": "^5.15.2",
    "@monaco-editor/react": "^4.3.1",
    "@prisma/client": "^3.9.0",
    "@radix-ui/react-tooltip": "^0.1.6",
    "@react-three/fiber": "^7.0.25",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/server-runtime": "^1.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "@vechaiui/core": "^0.6.0",
    "@vechaiui/react": "^0.2.1",
    "ahooks": "^3.1.8",
    "antd": "^4.18.5",
    "babel-preset-minify": "^0.5.1",
    "konva": "^8.3.2",
    "monaco-editor": "^0.31.1",
    "rc-drawer": "^4.4.3",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-konva": "^17.0.2-5",
    "recoil": "^0.5.2",
    "remix": "^1.1.1",
    "remix-utils": "^2.4.0",
    "three": "^0.136.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
